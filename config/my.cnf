[mysqld]
character-set-server=utf8mb4
default_storage_engine=InnoDB
log-error=/var/log/mysql/mysqld.log
log_timestamps=SYSTEM
slow_query_log=ON
slow_query_log_file=/var/log/mysql/slow_query.log
[client]
default-character-set=utf8mb4
[mysql]
default-character-set=utf8mb4
[mysqldump]
default-character-set=utf8mb4